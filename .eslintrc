{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "arrowFunctions": true,
      "binaryLiterals": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": true,
      "generators": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": true,
      "regexUFlag": true,
      "regexYFlag": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": true,
      "globalReturn": true,
      "jsx": true
    }
  },
  "rules": {
    "no-bitwise": 0,
    "camelcase": 2,
    "curly": 0,
    "eqeqeq": 0,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "wrap-iife": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 0,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 2,
    "max-params": [
      2,
      10
    ],
    "max-depth": [
      2,
      5
    ],
    "max-statements": [
      2,
      50
    ],
    "complexity": [
      2,
      8
    ],
    "max-len": [
      2,
      120
    ],
    "semi": 2,
    "no-cond-assign": 0,
    "no-debugger": 0,
    "no-eq-null": 2,
    "no-eval": 0,
    "no-unused-expressions": 0,
    "block-scoped-var": 0,
    "no-iterator": 0,
    "linebreak-style": [
      2,
      "unix"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "no-loop-func": 2,
    "no-multi-str": 2,
    "valid-typeof": 0,
    "no-proto": 0,
    "no-script-url": 0,
    "no-shadow": 2,
    "dot-notation": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-invalid-this": 0,
    "require-yield": 0,
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "if": {
            "after": false
          },
          "for": {
            "after": false
          },
          "while": {
            "after": false
          },
          "do": {
            "after": false
          },
          "switch": {
            "after": false
          },
          "catch": {
            "after": false
          }
        }
      }
    ],
    "no-with": 2,
    "brace-style": 2,
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-infix-ops": 2,
    "one-var": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "yoda": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never",
      {
        "singleValue": false
      }
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "quote-props": 0,
    "spaced-comment": [
      2,
      "always"
    ],
    "eol-last": 2,
    "no-multiple-empty-lines": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2
  }
}
